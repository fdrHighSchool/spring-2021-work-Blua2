public class Arraytester {
    public static int[] getColumn(int[][] arr2D, int c){
        int[] Fal = new int[arr2D.length];
        for (int i = 0; i < arr2D.length; i++){
            Fal[i] = arr2D[i][c];
        }
        return Fal;
    }

    public static boolean hasAllValues(int[] arr1, int[] arr2){
        boolean Y = false;
        if(arr1 == arr2){
            Y = true;
        }
        return Y;
    } 
}
